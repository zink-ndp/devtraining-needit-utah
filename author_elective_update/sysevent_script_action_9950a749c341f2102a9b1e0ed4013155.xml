    <sysevent_script_action action="DELETE">
        <active>false</active>
        <condition_script/>
        <description/>
        <event_name>activate.life.cycle.migration</event_name>
        <name>onLoginClick</name>
        <order>100</order>
        <script><![CDATA[function onLoginClick(event, api){
	const username = api.state.login-info.username;
	const password = api.state.login-info.password;

	if (!username || !password) {
		api.notifications.showToast({
			message: "Please enter username and password",
			variant: "error"
		});
		return;
	}

  // Gọi API login (REST API)
  api.data.loginApi.invoke({
    username,
    password
  });
}]]></script>
